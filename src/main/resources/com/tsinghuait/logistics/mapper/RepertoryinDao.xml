<?xml version="1.0" encoding="UTF-8" ?>
  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tsinghuait.logistics.mapper.RepertoryinDao">
	<insert id="insertRepertoryin" parameterType="repertoryin">
		insert into
		repertoryin(riId,orderNumber,riTimee,destination,checkstate)
		values(#{riId},#{orderNumber},#{riTimee},#{destination
		},#{checkstate})
	</insert>
</mapper>